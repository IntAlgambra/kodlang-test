{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "no-underscore-dangle": "off",
        "max-classes-per-file": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "no-plusplus": "off"
    }
}